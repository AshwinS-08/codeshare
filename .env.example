# Copy this file to `.env` and fill in the values for local development

# Flask
SECRET_KEY=dev-secret-key-change-in-production

# Supabase
SUPABASE_URL=https://YOUR-PROJECT.supabase.co
# Prefer service role key for admin checks (do NOT commit real keys)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional fallback keys if service role is not available
SUPABASE_KEY=your-key-or-anon-key
SUPABASE_ANON_KEY=your-anon-key

# Optional: override CORS origins (comma-separated)
# CORS_ORIGINS=http://localhost:3000,http://localhost:5173
